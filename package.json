{
  "name": "zodiac",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "zodiac": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:cli": "bun build src/cli/index.ts --outdir dist/cli --target node --format esm",
    "build:cli:bun": "bun build src/cli/index.ts --outdir dist/cli --target bun --format esm",
    "build:all": "bun run build && bun run build:cli",
    "cli:dev": "bun run src/cli/index.ts",
    "cli:install": "bun install",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:storybook": "vitest --project=storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
