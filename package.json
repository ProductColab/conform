    "@radix-ui/react-switch": "^1.2.5",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/vite": "^4.1.8",
    "@types/react-signature-canvas": "^1.0.7",
    "canvas2svg": "^1.0.16",
